type KeySpace {
    name: String!
    tables: [Table!]!
}

type Table {
    id: ID!
    name: String!
}

type Query {
    keyspaces: [KeySpace!]!
    tables(keyspace: String!): [Table!]!
}

input NewKeyspace {
    name: String!
}
input NewTable {
    name: String!
    params: [String]!
}

type Mutation {
    createTable(in: NewTable!): Table!
    dropTable(in: String!): Boolean!
}
